[Unit]
Description=Shortened URLs service - Minion worker (job queue)
Documentation=https://git.framasoft.org/luc/lstu
Requires=network.target
After=network.target

[Service]
Type=simple
User=www-data
RemainAfterExit=yes
WorkingDirectory=/var/www/lstu/
ExecStart=/usr/local/bin/carton exec script/application minion worker

[Install]
WantedBy=multi-user.target
